{
    "name": "division/configuration",
    "description": "Gestion de configurations pour laravel depuis une table de base de donn√©es",
    "authors": [
        {
            "name": "Ludovic Meurot",
            "email": "contact@ludovic-meurot.fr"
        }
    ],
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "Division\\Configurations\\": "src/",
            "Division\\Configurations\\Database\\Factories\\": "database/factories/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
